NATIVE_ENABLED = true
BYTE_ENABLED = true

USE_OCAMLFIND = true

OCAMLPACKS[] = unix pcre netstring bigarray
PRELUDE_OBJECTS[] =
  preArray
  preBigarray
  preBytestring
  preCombinators
  preFilesystem
  preIo
  preList
  prePath
  preRange
  preString
  preShell
